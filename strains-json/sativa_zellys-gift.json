norexia","score":5.5555555555555554},{"name":"Fibromyalgia","score":5.5555555555555554}],"negatives":[{"name":"Dry Mouth","score":25.555555555555557},{"name":"Dry Eyes","score":20}],"articlesAvailable":false,"articleTotalCount":0,"photos":[{"uploaded":"\/Date(-62135596800000)\/","thumb":"https://leafly-s3.s3.amazonaws.com/leafly/reviews/zelly-s-gift_100x100_f0b4.jpg","fullsize":"https://d3ix816x6wuc0d.cloudfront.net/cdn/strain-photo/1003017/b/zelly-s-gift__primary_6393.jpg"},{"uploaded":"\/Date(-62135596800000)\/","thumb":"https://leafly-s3.s3.amazonaws.com/leafly/reviews/zelly-s-gift_100x100_0459.jpg","fullsize":"https://d3ix816x6wuc0d.cloudfront.net/cdn/strain-photo/1001212/b/zelly-s-gift__primary_a156.jpg"},{"uploaded":"\/Date(-62135596800000)\/","thumb":"https://leafly-s3.s3.amazonaws.com/leafly/reviews/zelly-s-gift_100x100_0232.jpg","fullsize":"https://d3ix816x6wuc0d.cloudfront.net/cdn/strain-photo/1001211/b/zelly-s-gift__primary_7f61.jpg"}],"photoTotalCount":3,"popularCities":["Spokane, WA","Vancouver, BC","Calgary, AB","Casa Grande, AZ","Cathedral City, CA","Eugene, OR","Milwaukie, OR","Mohave Valley, AZ","Montréal, QC","Moses Lake, WA"],"permalink":"https://mobileapi.leafly.com/sativa/zellys-gift","starImage":"https://d3ix816x6wuc0d.cloudfront.net/stars/4.7/240","testGraph":null,"weakDescription":false,"videoUrl":null,"parents":[{"name":"Afgoo","subtitle":null,"slug":"afgoo","category":"Indica","permalink":"https://mobileapi.leafly.com/indica/afgoo","symbol":"Afo","applink":"leafly://strain/afgoo"},{"name":"Purple Thai","subtitle":null,"slug":"purple-thai","category":"Sativa","permalink":"https://mobileapi.leafly.com/sativa/purple-thai","symbol":"Pth","applink":"leafly://strain/purple-thai"},{"name":"Super Lemon Haze","subtitle":null,"slug":"super-lemon-haze","category":"Sativa","permalink":"https://mobileapi.leafly.com/sativa/super-lemon-haze","symbol":"Slh","applink":"leafly://strain/super-lemon-haze"},{"name":"God Bud","subtitle":null,"slug":"god-bud","ca